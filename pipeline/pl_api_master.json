{
	"name": "pl_api_master",
	"properties": {
		"activities": [
			{
				"name": "call_api",
				"description": "call api with pl parameters",
				"type": "ExecutePipeline",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "pl_api",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"SRC_REL_URL": {
							"value": "@concat(\n    pipeline().parameters.SRC_REL_PTH_START,\n    replace(pipeline().parameters.SRC_REL_PTH_MID, '#', '%23'),\n    pipeline().parameters.SRC_REL_PTH_END\n)",
							"type": "Expression"
						},
						"TGT_DIR": {
							"value": "@pipeline().parameters.TGT_DIR",
							"type": "Expression"
						},
						"TGT_FILE_NM": {
							"value": "@concat(\n    pipeline().parameters.TGT_FILE_NM,\n    '_',\n    utcnow(),\n    '.json'\n)",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"CLAN_TAG": {
				"type": "string",
				"defaultValue": "#P02CUUUU"
			},
			"SRC_REL_PTH_START": {
				"type": "string",
				"defaultValue": "clans/"
			},
			"SRC_REL_PTH_MID": {
				"type": "string",
				"defaultValue": "#P02CUUUU"
			},
			"SRC_REL_PTH_END": {
				"type": "string",
				"defaultValue": "''"
			},
			"TGT_FILE_NM": {
				"type": "string",
				"defaultValue": "clandata"
			},
			"TGT_DIR": {
				"type": "string",
				"defaultValue": "clash/clan/"
			}
		},
		"folder": {
			"name": "clash"
		},
		"annotations": []
	}
}