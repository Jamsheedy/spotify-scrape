{
	"name": "pullCurrentCWLWar",
	"properties": {
		"activities": [
			{
				"name": "pullCurrentCWL",
				"description": "call clash api /clans for given CLAN_TAG param",
				"type": "ExecutePipeline",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "pl_api",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"SRC_REL_URL": {
							"value": "@concat(\n    'clanwarleagues/wars/',\n    pipeline().parameters.WAR_TAG\n)",
							"type": "Expression"
						},
						"TGT_DIR": "clash/stg/cwl_wars/",
						"TGT_FILE_NM": {
							"value": "@concat(\n    'cwlwardata_',\n    utcnow(),\n    '.json'\n)",
							"type": "Expression"
						},
						"JOB_TIME": {
							"value": "@pipeline().parameters.JOB_TIME",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"WAR_TAG": {
				"type": "string",
				"defaultValue": "#88PG8C2QU"
			},
			"JOB_TIME": {
				"type": "string"
			}
		},
		"folder": {
			"name": "clash/pull/endpoints"
		},
		"annotations": []
	}
}