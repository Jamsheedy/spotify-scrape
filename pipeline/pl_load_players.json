{
	"name": "pl_load_players",
	"properties": {
		"activities": [
			{
				"name": "Get Players",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"additionalColumns": [
							{
								"name": "current_ts",
								"value": {
									"value": "@formatDateTime(utcnow(), 'yyyy.MM.dd HH:mm:ss')",
									"type": "Expression"
								}
							}
						],
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ds_sleeper",
						"type": "DatasetReference",
						"parameters": {
							"par_rel_url": {
								"value": "@'players/nfl'",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_json_gen",
						"type": "DatasetReference",
						"parameters": {
							"FILE_SYS": "raw",
							"DIR": "sleeper/players",
							"FILE_NM": {
								"value": "@concat(\n    'players_',\n    pipeline().parameters.par_league_id,\n    '_',\n    utcNow(),\n    '.json'\n)",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"par_league_id": {
				"type": "string",
				"defaultValue": "992125474790772736"
			}
		},
		"folder": {
			"name": "sleeper"
		},
		"annotations": []
	}
}